 <?php
 header('Content-Type: text/html; charset=utf-8'); 
 
$db = mysql_connect("localhost", "root", "") or die(mysql_error());
mysql_select_db("Supermarket");

//mysql_query("SET character_set_results = 'utf8', character_set_client = 'utf8', character_set_connection = 'utf8', character_set_database = 'utf8', character_set_server = 'utf8'", $db);
//tאם מפעילים את זה זה מראה עברית בבסיס נתונים אבל לא באתר. כשמורידים זה מראה באתר ולא בבסיס..

//mysql_query("SET character_set_server = utf8");
  //Creating the DB if not exist
	mysql_query("drop SCHEMA `Supermarket`;") ;
	mysql_query("CREATE DATABASE IF NOT EXISTS `Supermarket` DEFAULT CHARACTER SET 'utf8' ;") ;
	
	//Set using selected DB
	mysql_query("USE `Supermarket`;");
	mysql_query("SET character_set_server = utf8");
	
				 
	mysql_query("CREATE TABLE IF NOT EXISTS `users` (
				  `userName` varchar(50) NOT NULL,
				  `password` varchar(100) DEFAULT NULL,
				  `email` varchar(45) DEFAULT NULL,
				  `first` varchar(45) DEFAULT NULL,
				  `last` varchar(45) DEFAULT NULL,
				  `address` varchar(45) DEFAULT NULL,
				  `userNumber` varchar(45) NOT NULL,
				  `phone` varchar(45) DEFAULT NULL,
				  `phone2` varchar(45) DEFAULT NULL,
				  `valid` tinyint(4) DEFAULT '0',
				  `isEMPLOYEE` tinyint(4) DEFAULT '0',
				  `isMANAGER` tinyint(4) DEFAULT '0',
				  `loginStatus` smallint(6) DEFAULT '0',
				  PRIMARY KEY (`userName`)) ENGINE=InnoDB DEFAULT CHARSET=utf8");
				  
				  	
				
	mysql_query("CREATE TABLE IF NOT EXISTS `orders` (
			  `orderNumber` int(11) NOT NULL AUTO_INCREMENT,
			  `userName` varchar(40) NOT NULL,
			  `first` varchar(45) NOT NULL,
			  `last` varchar(45) NOT NULL,
			  `orderDate` datetime NOT NULL,
			  `orderSelectedTime` varchar(20) NOT NULL,
			  `TotalPrice` varchar(20) Not Null Default 0,
			  `orderStatus` tinyint(1) NOT NULL DEFAULT '3',
			  PRIMARY KEY (`orderNumber`)
			) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1");
					
	mysql_query("  CREATE TABLE IF NOT EXISTS `productsInOrder` (
					`proNumber` INT NOT NULL,
					`orderNumber` INT NOT NULL,
					`proQuantity` varchar(4) DEFAULT NULL,
					`proName` varchar(40) DEFAULT NULL,
					`price` varchar(20)  DEFAULT NULL,
					  KEY `proNumber` (`proNumber`),
					  KEY `orderNumber` (`orderNumber`)
					) ENGINE=InnoDB DEFAULT CHARSET=utf8");
					
	mysql_query("ALTER TABLE `productsinorder`
					  ADD CONSTRAINT `productsinorder_ibfk_2` FOREIGN KEY (`orderNumber`) REFERENCES `orders` (`orderNumber`),
					  ADD CONSTRAINT `productsinorder_ibfk_1` FOREIGN KEY (`proNumber`) REFERENCES `product` (`prodnumber`)
					  ) ENGINE=InnoDB DEFAULT CHARSET=utf8");


   mysql_query("CREATE  TABLE IF NOT EXISTS `product` (
  `prodnumber` INT NOT NULL AUTO_INCREMENT,
  `prodname` varchar(200)  DEFAULT NULL,
  `price` varchar(20)  DEFAULT NULL,
  `catagory` enum('ביצים','חלב', 'שימורים', 'ממתקים','ירקות', 'ממרחים', 'בשרים\דגים', 'דגני בוקר',  'חלב' , 'גלידה', 'לחם', 'משקאות', 'מוצרי הגיינה', 'חומרים וכלי ניקוי', 'פירות', 'קפואים', 'אלקטרוניקה וחשמל', 'תבלינים', 'ציוד משרדי', 'מוצרי אמבטיה', 'בגדים דלתא', 'מרקים')NOT NULL,
  `status` TINYINT NULL DEFAULT 1 ,
  `image` LONGBLOB NOT NULL,
	PRIMARY KEY (`prodnumber`) )ENGINE=InnoDB DEFAULT CHARSET=utf8");
				 
  mysql_query("CREATE TABLE IF NOT EXISTS `sale` (
			  `SaleNumber` int(40) NOT NULL AUTO_INCREMENT,
			  `prodnumber` int(40) NOT NULL,
			  `prodname` varchar(50) NOT NULL,
			  `description` varchar(50) NOT NULL,
			  `status` tinyint(1) NOT NULL DEFAULT '1',
			  PRIMARY KEY (`SaleNumber`)
			) ENGINE=InnoDB DEFAULT CHARSET=utf8 AUTO_INCREMENT=1 ");

